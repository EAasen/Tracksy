{
  "name": "tracksy-frontend",
  "version": "1.0.0",
  "description": "Frontend web application for Tracksy",
  "main": "index.html",
  "scripts": {
    "start": "webpack serve --env development --progress",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:analyze": "ANALYZE=true webpack --mode production",
    "serve:dist": "npx serve -s dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write 'src/**/*.{js,jsx,css}'",
    "security:audit": "npm audit --audit-level=moderate || exit 0",
    "security:fix": "npm audit fix || exit 0"
  },
  "author": "Eirik Aasen",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^1.6.8",
    "chart.js": "^3.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^18.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^8.0.1",
    "css-loader": "^5.2.6",
    "postcss-loader": "^6.1.0",
    "react-refresh": "^0.14.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.2.4",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^5.0.4"
  }
}
